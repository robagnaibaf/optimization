    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "%matplotlib inline\n",
    "\n",
    "n=100;\n",
    "x=np.linspace(0,2*np.pi,n);\n",
    "y=np.sin(3*x)*np.exp(-x);\n",
    "plt.plot(x,y)\n",
    "plt.draw()"
   ]
